<frw:UniWindow
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:frw="clr-namespace:Framework.Common;assembly=Framework.Common"
    xmlns:local="clr-namespace:UniClient"
    x:Class="UniClient.LoginWindow"
    Height="270" Width="300" CanResize="False"
    WindowStartupLocation="CenterScreen">
    
    <frw:UniWindow.LeftContent>
        <local:TitleBarLeftView/>
    </frw:UniWindow.LeftContent>
    
    <frw:UniWindow.RightContent>
        <local:TitleBarRightView/>
    </frw:UniWindow.RightContent>
    
    <local:LoginView x:Name="LoginView" Margin="0,10,0,0"/>
    
</frw:UniWindow>
